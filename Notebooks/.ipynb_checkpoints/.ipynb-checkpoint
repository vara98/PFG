    "dictionaries = []\n",
    "\n",
    "for columna in ['Ganancia','Dia']:\n",
    "    datos[columna], uniques = pd.factorize(datos[columna])\n",
    "    values = list(range(len(uniques)))\n",
    "    dictionaries.append(dict(zip(values, uniques)))"
